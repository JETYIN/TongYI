<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.qianseit.westore.ui.CommonButton
        android:id="@+id/unifiedorder_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="APP支付生成预支付订单" />

    <com.qianseit.westore.ui.CommonButton
        android:id="@+id/appay_pre_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="生成APP微信支付参数" />

    <com.qianseit.westore.ui.CommonButton
        android:id="@+id/appay_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="调起微信支付" />

    <com.qianseit.westore.ui.CommonTextView
        android:id="@+id/editText_prepay_id"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </com.qianseit.westore.ui.CommonTextView>

</LinearLayout>